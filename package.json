{
  "name": "next-js-starter",
  "version": "0.1.0",
  "description": "Next.js Web Starter",
  "private": true,
  "author": "",
  "license": "MIT",
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint '*/**/*.{js,jsx,ts,tsx}' --quiet",
    "format": "prettier --write '*/**/*.{js,jsx,ts,tsx,json,css,md}'",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit && echo ",
    "lint-staged": "npm run precommit-msg && eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run precommit-msg && eslint --fix . --ext src/.js,.jsx,.ts,.tsx",
    "precommit-msg": "echo 'Web: Pre-commit lint checks...' && exit 0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "next": "14.1.0",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "typescript": "^5"
  }
}
