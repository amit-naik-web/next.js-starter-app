{
  "name": "next-js-starter",
  "version": "0.1.0",
  "description": "Next.js Web Starter",
  "private": true,
  "author": "Amit Naik",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --check",
      "git add"
    ]
  },
  "pre-commit": [
    "pre-commit-msg",
    "lint"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write '*/**/*.{js,jsx,ts,tsx,json,css,md}'",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit && echo",
    "lint": "npm run pre-commit-msg && eslint . --ext src/.js,.jsx,.ts,.tsx",
    "lint:fix": "npm run pre-commit-msg && eslint --fix . --ext src/.js,.jsx,.ts,.tsx",
    "pre-commit-msg": "echo 'Web: Pre-commit lint checks...' && exit 0",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.10",
    "@mui/material-nextjs": "^5.15.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "next": "14.1.0",
    "next-intl": "^3.9.1",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "lint-staged": "^15.2.2",
    "typescript": "^5"
  }
}
